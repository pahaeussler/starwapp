<p id="notice"><%= notice %></p>

<h1>Starships</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <%# <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @starships.each do |starship| %>
      <tr>
        <td><%= link_to starship['name'], starship_path(get_id(starship['url'], 'starships')) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
